{
  "name": "graphql-api-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "export DEBUG=* && npm run codegen && nodemon -r dotenv/config src/index.ts --exec ts-node src/index.ts -e ts,graphql,json",
    "build": "npm run codegen && tsc -p . && cp -r src/graphql/schemas dist/graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "echo \"Error: no tests specified\" && exit 0",
    "serve": "export NODE_ENV=production && node dist/index.js"
  },
  "keywords": [
    "graphql",
    "api"
  ],
  "author": "DUCKMADE <hello@duckmade.io>",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "2.5.0",
    "@graphql-codegen/typescript-resolvers": "^2.6.5",
    "@graphql-tools/schema": "^8.3.14",
    "@graphql-tools/utils": "^8.6.13",
    "@types/compression": "^1.7.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/simple-oauth2": "^4.1.1",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "apollo-datasource-http": "^0.21.0",
    "apollo-server-core": "^3.8.2",
    "apollo-server-express": "^3.8.2",
    "compression": "^1.7.4",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "graphql": "^16.5.0",
    "graphql-import-node": "^0.0.5",
    "http": "^0.0.1-security",
    "lodash": "^4.17.21",
    "oauth2": "^0.0.1",
    "simple-oauth2": "^4.3.0",
    "undici": "^5.4.0",
    "winston": "^3.7.2"
  }
}
