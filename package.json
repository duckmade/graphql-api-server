{
  "name": "duckmade-graphql-api-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run codegen && nodemon src/index.ts --exec ts-node src/index.ts -e ts,graphql,json",
    "build": "npm run codegen && tsc -p . && cp -r src/graphql/schemas dist/graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "pm2 start dist/index.js"
  },
  "keywords": [
    "graphql"
  ],
  "author": "DUCKMADE",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-resolvers": "2.6.4",
    "@graphql-tools/schema": "^8.3.13",
    "@graphql-tools/utils": "^8.6.12",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "apollo-server-express": "^3.8.1",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-import-node": "^0.0.5",
    "lodash": "^4.17.21"
  }
}
